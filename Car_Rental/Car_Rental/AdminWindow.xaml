<Window x:Class="Car_Rental.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Panel Administratora" Height="350" Width="500" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="20">
        <TextBlock Text="Panel Administratora" FontSize="22" FontWeight="Bold" Margin="0,0,0,20"/>
        <Button Content="Dodaj rezerwację" Margin="0,0,0,10" Height="35"/>
        <Button Content="Wyświetl dostępne auta" Margin="0,0,0,10" Height="35"/>
        <Button Content="Zarządzaj użytkownikami" Margin="0,0,0,10" Height="35" x:Name="AddEmployeeButton" Click="AddEmployeeButton_Click"/>
        <Button Content="Raporty" Margin="0,0,0,10" Height="35"/>
        <Button Content="Wyloguj" Margin="0,20,0,0" Height="35"/>
    </StackPanel>
</Window>